<div id="youtube-videos">
  <% videos.each do |video| %>
    <p>
    <iframe width="400" height="300" src="http://www.youtube.com/embed/<%= video.video_id %>?controls=1" allowfullscreen="allowfullscreen"></iframe></br>
      <%= link_to video.title, video.url %></br>
      <%= "#{video.published_at.strftime("%B %d, %Y %l:%M %p")},  #{time_ago_in_words(video.published_at)} ago" %>
    </p>
  <% end %>
</div>
