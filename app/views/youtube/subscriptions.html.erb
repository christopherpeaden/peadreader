<%= link_to "Refresh Access Token", refresh_access_token_path %>
<%= @new_token_info %>

<% @channels.each do |channel| %>
  <p><%= link_to image_tag(channel.image), channel.url %></p>
  <p><%= link_to channel.title, channel.url %></p>
<% end %>
  

<% @videos.each do |video| %>
  <p><%= link_to image_tag(video.image), video.url %></p>
  <p><%= link_to video.title, video.url %></p>
  <p><%= video.published_at.strftime("%B %d, %Y %l:%M %p") %></p>
<% end %>
