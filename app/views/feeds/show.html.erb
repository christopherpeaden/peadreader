<div class="heading">
  <h1><%= @feed.title %></h1>
    <%= link_to "Edit", edit_feed_path(@feed), class: "btn" %>
    <%= link_to "Delete", @feed, method: :delete, data: { confirm: "Delete this feed?" }, class: "btn" %>
    <%= link_to "Back", root_path, class: "btn" %>
</div>

<div id="items">
  <%= render "items", items: @items %>
</div>

<%= will_paginate @items %>
