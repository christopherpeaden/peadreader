<div id="sidebar">
  <ul class="list-group">

    <% Category.all.each do |category| %>
      <li><%= link_to category.title, root_path(category_id: category.id) %><span class="caret">
        <ul>
          <% category.feeds.each do |feed| %>
            <li><%= link_to feed.title, feed %></li>
          <% end %>
      </ul>
    </span>
      </li>
  <% end %>

  </ul>
</div>
